#!/usr/bin/env python

import functools, untwisted
from testify import *
from untwisted import event

@test
def callThenShift():
  expect(1)

  sample = event.sequence()
  sample('Expect')

  sample.shift().connect(functools.partial(equal, 'Expect'))

@test
def shiftThenCall():
  expect(1)

  sample = event.sequence()

  sample.shift().connect(functools.partial(equal, 'Expect'))

  sample('Expect')
