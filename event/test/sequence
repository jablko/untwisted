#!/usr/bin/env python

import untwisted
from testify import *
from untwisted import event

@test
def callThenShift():
  expect(1)

  sample = event.sequence()
  sample('Expect')

  sample.shift().connect(lambda actual: equal('Expect', actual))

@test
def shiftThenCall():
  expect(1)

  sample = event.sequence()

  sample.shift().connect(lambda actual: equal('Expect', actual))

  sample('Expect')
